{
  "expo": {
    "name": "Worthy I",
    "slug": "worthy-i",
    "version": "1.0.1",
    "runtimeVersion": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/Worthy-I-logo.png",
    "scheme": "worthyi",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Worthy-I-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "imageResizeMode": "contain",
      "imageSize": "70%"
    },
    "jsEngine": "hermes",
    "ios": {
      "bundleIdentifier": "com.worthyilife.thanks",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "monochromeImage": "./assets/images/android-adaptive-icon.png"
      },
      "package": "com.worthyilife.thanks"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "worthyi",
    "sdkVersion": "52.0.0",
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_LOAD",
      "url": "https://u.expo.dev/ba041c45-0229-437a-8d3e-81aba979dd5b"
    },
    "extra": {
      "eas": {
        "projectId": "ba041c45-0229-437a-8d3e-81aba979dd5b"
      }
    }
  }
}
